<view wx:for="{{expertlist}}" wx:key="expertses">
  <view class="page-main" style="margin: 15rpx;">
    <view class="weui-media-box__hd">
      <image class="page-main-img" src="{{item.img}}" />
    </view>
    <view class="weui-panel__bd" style="margin-left: -10px;">
      <view class="weui-media-box weui-media-box_text">
        <view class="weui-media-box__title">{{item.name}}</view>
        <view class="weui-media-box__desc">{{item.info}}</view>
        <view class="weui-media-box__info">
          <view class="weui-media-box__info__meta" style="color: #888;">评分</view>
          <view class="weui-media-box__info__meta weui-media-box__info__meta_extra page-main-rate">{{item.point}}</view>
        </view>
      </view>
    </view>
  </view>
</view>